{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "config": {
            "type": "object",
            "properties": {
                "app": {
                    "type": "object",
                    "properties": {
                        "port": {
                            "type": "integer"
                        }
                    }
                },
                "rplan_config_webclient": {
                    "type": "object",
                    "properties": {
                        "intercom_app_id": {
                            "type": "null"
                        },
                        "intercom_enabled": {
                            "type": "boolean"
                        },
                        "intercom_widget_url": {
                            "type": "null"
                        },
                        "rest_api_host": {
                            "type": "string"
                        },
                        "rest_api_protocol": {
                            "type": "string"
                        },
                        "sentry_dsn": {
                            "type": "null"
                        },
                        "sentry_enabled": {
                            "type": "boolean"
                        },
                        "sentry_environment": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "logLevel": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object"
        },
        "podAnnotations": {
            "type": "object"
        },
        "podLabels": {
            "type": "object"
        },
        "replicaCount": {
            "type": "integer"
        },
        "resources": {
            "type": "object"
        },
        "service": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "labels": {
                    "type": "object"
                },
                "name": {
                    "type": "string"
                },
                "port": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
