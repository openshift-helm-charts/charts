    {
      "datasource": { "type": "victoriametrics-metrics-datasource", "uid": "${datasource}" },
      "description": "Distribution of HTTP server request durations over time.",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "scaleDistribution": { "type": "linear" }
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 3 },
      "id": 7,
      "options": {
        "calculate": false,
        "cellGap": 1,
        "color": {
          "exponent": 0.5,
          "fill": "dark-orange",
          "mode": "scheme",
          "reverse": false,
          "scale": "exponential",
          "scheme": "RdYlGn",
          "steps": 64
        },
        "exemplars": { "color": "rgba(255,0,255,0.7)" },
        "filterValues": { "le": 1e-9 },
        "legend": { "show": true },
        "rowsFrame": { "layout": "auto" },
        "tooltip": { "mode": "single", "showColorScale": false, "yHistogram": false },
        "yAxis": { "axisPlacement": "left", "reverse": false, "unit": "ms" }
      },
      "pluginVersion": "11.3.0",
      "targets": [
        {
          "datasource": { "type": "victoriametrics-metrics-datasource", "uid": "${datasource}" },
          "editorMode": "code",
          "expr": "prometheus_buckets(sum(rate(http.server.duration_bucket{service.name=~\"$service\", http.target=~\"$route\"}[5m])) by(vmrange))",
          "format": "heatmap",
          "instant": false,
          "legendFormat": "__auto",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "HTTP Server Duration",
      "type": "heatmap"
    },

    {
      "datasource": { "type": "victoriametrics-metrics-datasource", "uid": "${datasource}" },
      "description": "Requests per second (RPS) and error rates. Error series use right axis (percent).",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "axisPlacement": "left" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "4xx %" },
            "properties": [
              { "id": "unit", "value": "percentunit" },
              { "id": "custom.axisPlacement", "value": "right" }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "5xx %" },
            "properties": [
              { "id": "unit", "value": "percentunit" },
              { "id": "custom.axisPlacement", "value": "right" }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "RPS" },
            "properties": [{ "id": "unit", "value": "reqps" }]
          }
        ]
      },
      "gridPos": { "h": 5, "w": 12, "x": 12, "y": 3 },
      "id": 11,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "pluginVersion": "11.3.0",
      "targets": [
        {
          "datasource": { "type": "victoriametrics-metrics-datasource", "uid": "${datasource}" },
          "editorMode": "code",
          "expr": "sum(rate(http.server.duration_count{service.name=~\"$service\", http.target=~\"$route\"}[1m]))",
          "legendFormat": "RPS",
          "refId": "A"
        },
        {
          "datasource": { "type": "victoriametrics-metrics-datasource", "uid": "${datasource}" },
          "editorMode": "code",
          "expr": "sum(rate(http.server.duration_count{service.name=~\"$service\", http.target=~\"$route\", http.status_code=~\"4..\"}[1m]))\n/\nsum(rate(http.server.duration_count{service.name=~\"$service\", http.target=~\"$route\"}[1m]))",
          "legendFormat": "4xx %",
          "refId": "B"
        },
        {
          "datasource": { "type": "victoriametrics-metrics-datasource", "uid": "${datasource}" },
          "editorMode": "code",
          "expr": "sum(rate(http.server.duration_count{service.name=~\"$service\", http.target=~\"$route\", http.status_code=~\"5..\"}[1m]))\n/\nsum(rate(http.server.duration_count{service.name=~\"$service\", http.target=~\"$route\"}[1m]))",
          "legendFormat": "5xx %",
          "refId": "C"
        }
      ],
      "title": "RPS & Error %",
      "type": "timeseries"
    },

